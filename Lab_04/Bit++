#include<bits/stdc++.h>
#include<map>
#include <algorithm>
#include<vector>
#include<map>
#include<string>
using namespace std;
int main()
{
   int t;
   cin>>t;
   int a=0;
   while(t--){
    string s;
    cin>>s;
    if(s[0]=='+' || s[1]=='+'){
        a++;
    }
    else if(s[0]=='-' || s[1]=='-'){
        a--;
    }
 
   }
   cout<<a<<endl;
 
    return 0;
}
