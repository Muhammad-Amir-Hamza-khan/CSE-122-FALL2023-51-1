#include<bits/stdc++.h>
#include<map>
#include <algorithm>
#include<vector>
#include<map>
#include<string>
using namespace std;
int main()
{
    int n;
   int a[5][5];
   for(int i=0;i<5;i++){
       for(int j=0;j<5;j++){
           cin>>a[i][j];
           if(a[i][j]==1){
               n=abs(i-2)+abs(j-2);
           }
       }
   }
   cout<<n<<endl;

    return 0;
}
