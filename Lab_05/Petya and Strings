#include<bits/stdc++.h>
#include<map>
#include <algorithm>
#include<vector>
#include<map>
#include<string>
using namespace std;
int main()
{
    string s,a;
    cin>>s>>a;
    for(int i=0;i<s.length();i++){
        s[i]=tolower(s[i]);
    }
    for(int i=0;i<a.length();i++){
        a[i]=tolower(a[i]);
    }
    if(s==a){
        cout<<"0"<<endl;
    }
    else if(s>a){
        cout<<"1"<<endl;
    }
    else{
        cout<<"-1"<<endl;
    }
 
    return 0;
}
